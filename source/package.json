{
  "name": "celerik-scaffolder",
  "displayName": "Celerik scaffolder",
  "description": "Extension for VSCode that helps to automate code generation through scaffolding",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webview.open",
        "title": "Open Webview",
        "category": "Webview React"
      },
      {
        "command": "extension.sendMessage",
        "title": "Send Message to Webview",
        "category": "Webview React"
      }
    ],
    "configuration": {
      "title": "Webview React",
      "properties": {
        "webviewReact.userApiGender": {
          "type": "string",
          "default": "male",
          "enum": [
            "male",
            "female"
          ],
          "enumDescriptions": [
            "Fetching user information with gender of male",
            "Fetching user information with gender of female"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/mocha": "9.1.1",
    "@types/node": "18.0.0",
    "@types/react": "18.0.13",
    "@types/react-dom": "18.0.5",
    "@types/vscode": "1.68.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "css-loader": "6.7.1",
    "eslint": "8.17.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "8.0.3",
    "mocha": "10.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.0",
    "typescript": "4.7.3",
    "vscode-test": "1.4.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  }
}
